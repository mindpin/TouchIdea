<div class="form-actions">
  <%= link_to "一键登录，贡献我的想法", omniauth_authorize_path(:user, :weibo), class: 'btn-lg btn-primary btn-block text-center' %><br />
</div>

<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title text-center">参与议题讨论，贡献我的想法</h3>
  </div>
  <div class="panel-body">
    <h1 class="text-center">议题：<%= @vote.title %></h1>
    <%= text_field_tag nil, token_url(@vote.token), class: 'form-control', disabled: true %>
  </div>
</div>

<div id="vote-form" class="">
  <%= render "show_form_disabled", vote: @vote %>
</div>

